      "created": 1643845427,
      "updated": 1643845427
    },
    {
      "id": "61fb17336eb4060000d9a693",
      "text": "  const emojiCompletion = new EmojiCompletion({",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1643845427,
      "updated": 1643845427
    },
    {
      "id": "61fb17336eb4060000d9a694",
      "text": "   \t\t\t\t\t\t\tprojects: ['icons','emoji','icons2'],",
      "userId": "58442db3ee4a2400116eb406",
      "created": 1643845427,
      "updated": 1643845427
    },
    {
      "id": "61fb17336eb4060000d9a695",
