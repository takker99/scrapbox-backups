  "lines": [
    {
      "id": "5fd5e4e0af9b05001d4398ae",
      "text": "ニーバーの祈り",
      "userId": "5fa258bdb102120024d43875",
      "created": 1607853346,
      "updated": 1607853346
    },
    {
      "id": "5fd5e52ad4387500001e393a",
      "text": "変えられないものを受け入れる力を",
      "userId": "5fa258bdb102120024d43875",
      "created": 1607853354,
      "updated": 1607853368
    },
    {
      "id": "5fd5e522d4387500001e3938",
      "text": "変えられるものを変える勇気を",
      "userId": "5fa258bdb102120024d43875",
      "created": 1607853346,
      "updated": 1607853352
    },
    {
      "id": "5fd5e526d4387500001e3939",
      "text": "変えられないものと変えるべきものを区別する賢さを",
      "userId": "5fa258bdb102120024d43875",
      "created": 1607853350,
      "updated": 1607853422
    },
    {
      "id": "5fd5e580d4387500001e393d",
      "text": "",
      "userId": "5fa258bdb102120024d43875",
      "created": 1607853440,
      "updated": 1607853440
    },
    {
      "id": "5fd5e580d4387500001e393e",
      "text": "[影響の輪]",
      "userId": "5fa258bdb102120024d43875",
      "created": 1607853440,
      "updated": 1607853444
    },
    {
      "id": "5fd5e586d4387500001e393f",
      "text": "",
      "userId": "5fa258bdb102120024d43875",
      "created": 1607853446,
      "updated": 1607853446
    },
