    {
      "id": "5fc5fbc0d438750000d4452e",
      "text": "code:oyasumi.py",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810561,
      "updated": 1606810565
    },
    {
      "id": "5fc5fbc4d438750000d44530",
      "text": " import schedule",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810565,
      "updated": 1606810565
    },
    {
      "id": "5fc5fbc4d438750000d44531",
      "text": " import time",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810565,
      "updated": 1606810565
    },
    {
      "id": "5fc5fbc4d438750000d44532",
      "text": " import os",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810565,
      "updated": 1606810565
    },
    {
      "id": "5fc5fbc4d438750000d44533",
      "text": " ",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810565,
      "updated": 1606810565
    },
    {
      "id": "5fc5fbc4d438750000d44535",
      "text": " def suspend():",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810565,
      "updated": 1606810565
    },
    {
      "id": "5fc5fbc4d438750000d44536",
      "text": "     os.system(\"systemctl suspend\")",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810565,
      "updated": 1606810565
    },
    {
      "id": "5fc5fbc4d438750000d44537",
      "text": " ",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810565,
      "updated": 1606810565
    },
    {
      "id": "5fc5fbc4d438750000d44539",
      "text": " schedule.every().day.at(\"21:30\").do(suspend)",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810565,
      "updated": 1606810565
    },
    {
      "id": "5fc5fbc4d438750000d4453a",
      "text": " ",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810565,
      "updated": 1606810591
    },
    {
      "id": "5fc5fbded438750000d44541",
      "text": " while True:",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810591,
      "updated": 1606810591
    },
    {
      "id": "5fc5fbc4d438750000d4453b",
      "text": "     schedule.run_pending()",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810565,
      "updated": 1606810565
    },
    {
      "id": "5fc5fbc4d438750000d4453c",
      "text": "     time.sleep(1)",
      "userId": "5fa258bdb102120024d43875",
      "created": 1606810565,
      "updated": 1606810565
    },
    {
      "id": "5fc5fbc4d438750000d4453d",
