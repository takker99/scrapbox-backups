      "created": 1610442905,
      "updated": 1610442915
    },
    {
      "id": "601362d1d4387500004911b1",
      "text": " import os",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611883218,
      "updated": 1611883220
    },
    {
      "id": "60136394d438750000c2a797",
      "text": " os.system(\"touch ~/.bashrc\")",
      "userId": "5fa258bdb102120024d43875",
      "created": 1611883412,
      "updated": 1611883423
    },
    {
      "id": "5ffd68a3d43875000096c1ed",
      "text": " path = '~/.bashrc'",
      "userId": "5fa258bdb102120024d43875",
      "created": 1610442915,
      "updated": 1610442915
    },
    {
      "id": "5ffd68a3d43875000096c1ee",
      "text": " ",
      "userId": "5fa258bdb102120024d43875",
      "created": 1610442915,
      "updated": 1610442969
    },
    {
      "id": "5ffd68d9d43875000096c1fa",
      "text": " short=input(\"Short expression?\")",
      "userId": "5fa258bdb102120024d43875",
      "created": 1610442969,
      "updated": 1610442969
    },
    {
      "id": "5ffd68a3d43875000096c1f1",
      "text": " full=input(\"Full expression?\")",
      "userId": "5fa258bdb102120024d43875",
      "created": 1610442915,
      "updated": 1610442963
    },
    {
      "id": "5ffd68a3d43875000096c1f3",
      "text": " ",
      "userId": "5fa258bdb102120024d43875",
      "created": 1610442915,
      "updated": 1610442970
    },
    {
      "id": "5ffd68dad43875000096c1fb",
      "text": " newline = \"\\nalias \"+short+\"='\"+full+\"'\"",
      "userId": "5fa258bdb102120024d43875",
      "created": 1610442970,
      "updated": 1610442970
    },
    {
      "id": "5ffd68a3d43875000096c1f4",
      "text": " ",
      "userId": "5fa258bdb102120024d43875",
      "created": 1610442915,
      "updated": 1610442915
    },
    {
      "id": "5ffd68a3d43875000096c1f6",
      "text": " with open(path,'a') as f:",
      "userId": "5fa258bdb102120024d43875",
      "created": 1610442915,
      "updated": 1610442915
    },
    {
      "id": "5ffd68a3d43875000096c1f7",
      "text": "         f.write(newline)",
      "userId": "5fa258bdb102120024d43875",
      "created": 1610442915,
      "updated": 1610442915
    },
    {
      "id": "5ffd68a3d43875000096c1f9",
      "text": "         print(\"Alias added!!\")",
      "userId": "5fa258bdb102120024d43875",
      "created": 1610442915,
      "updated": 1610442915
    },
    {
      "id": "5ffd689ed43875000096c1ec",
      "text": "\tos.system(\"source ~/.bashrc\")",
      "userId": "5fa258bdb102120024d43875",
      "created": 1610442911,
      "updated": 1611883354
    },
    {
      "id": "601362e7d4387500002cde36",
